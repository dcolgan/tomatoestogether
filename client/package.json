{
  "name": "tomatoestogether-client",
  "version": "3.0.0",
  "description": "Be productive together!",
  "main": "main.coffee",
  "scripts": {
    "start": "npm run watch & npm run css & npm run static",
    "css": "stylus -w style.styl -o assets/style.css",
    "static": "http-server assets/ -p 8000 -s",
    "watch": "watchify src/main.coffee -g coffeeify --fast --extension .js --extension .coffee -o assets/bundle.js -dv",
    "build": "browserify src/main.coffee -g coffeeify --fast --extension .js --extension .coffee | uglifyjs > assets/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dvcolgan/tomatoestogether"
  },
  "author": "David Colgan",
  "license": "MIT",
  "dependencies": {
    "browserify": "^13.1.0",
    "coffee-script": "^1.10.0",
    "coffeeify": "^2.0.1",
    "http-server": "^0.9.0",
    "mithril": "^0.2.5",
    "mocha": "^3.0.2",
    "socket.io-client": "^1.4.8",
    "stylus": "^0.54.5",
    "tape": "^4.6.0",
    "watchify": "^3.7.0"
  }
}
