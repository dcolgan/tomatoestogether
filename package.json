  "dependencies": {
    "coffee-script": "^1.10.0",
    "deodorant": "^2.1.0",
    "mithril": "^0.2.0",
    "express": "^4.13.3",
    "socket.io": "^1.3.7",
    "socket.io-client": "^1.3.7",
    "node-html-encoder": "0.0.2"
  },
  "devDependencies": {
    "browserify": "^11.2.0",
    "coffeeify": "^1.1.0",
    "node-dev": "^2.7.1",
    "watchify": "^3.4.0"
  }
