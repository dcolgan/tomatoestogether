{
  "name": "tomatoestogether",
  "version": "0.0.1",
  "description": "The Tomatoes Together Frontend Website",
  "scripts": {
    "start": "npm run watch-client & npm run watch-scss & npm run livereload",
    "watch-client": "watchify --debug index.ts -p tsify -o assets/bundle.js",
    "watch-scss": "nodemon -e scss -x \"npm run build-scss\"",
    "build-scss": "node-sass --include-path=scss scss/main.scss assets/styles.css",
    "livereload": "livereload assets/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dvcolgan/tomatoestogether.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dvcolgan/tomatoestogether/issues"
  },
  "homepage": "https://www.tomatoestogether.com",
  "author": "David Colgan",
  "dependencies": {
    "@types/axios": "^0.9.34",
    "@types/es6-promise": "0.0.32",
    "@types/marked": "0.0.28",
    "@types/moment": "^2.13.0",
    "@types/node-uuid": "0.0.28",
    "@types/react": "^0.14.54",
    "@types/react-dom": "^0.14.19",
    "@types/react-router": "^3.0.3",
    "@types/victory": "^0.9.5",
    "@types/xtend": "^4.0.2",
    "awesome-typescript-loader": "^3.0.8",
    "axios": "^0.15.3",
    "bootstrap": "^4.0.0-alpha.6",
    "browserify": "^13.1.1",
    "css-loader": "^0.26.2",
    "es6-promise": "^4.0.5",
    "import-glob": "^1.5.0",
    "livereload": "^0.6.0",
    "marked": "^0.3.6",
    "mobx": "^2.7.0",
    "mobx-react": "^4.1.0",
    "moment": "^2.17.1",
    "node-sass": "^4.3.0",
    "nodemon": "^1.11.0",
    "react": "^15.4.1",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.2",
    "react-router-restore-scroll": "0.0.1",
    "react-spin": "^0.6.2",
    "sass-loader": "^6.0.2",
